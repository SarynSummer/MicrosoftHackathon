<div class="mainContainer">
        <a href="/">Go To Home</a>
    {{#each data}}
    <span class="eachResult">
        <p class="resultText">
            <h6>recordID: {{_id}} <br />
                recorded: {{ recorded }}</h6>
            <div class="row">
                <div class="col">
                    <strong>Face Scan Data:</strong> <br />
                    <div class="row">
                        <div class="col">
                            anger {{anger}} <br />
                            contempt {{contempt}} <br />
                            disgust {{disgust}} <br />
                            fear {{fear}} <br />
                            happiness {{happiness}} <br />
                            neutral {{neutral}} <br />
                        </div>
                        <div class="col">
                            sadness {{sadness}} <br />
                            surprise {{surprise}} <br />
                            eyeMakeup {{eyeMakeup}} <br />
                            lipMakeup {{lipMakeup}} <br />
                            moustache {{moustache}} <br />
                            beard {{beard}} <br />
                            smile {{smile}} <br />
                        </div>
                    </div>
                </div>
                <div class="col">
                    <strong>Survey Questions: </strong><br />
                    QOne:  {{questionOne}} <br />
                    QTwo: {{questionTwo}} <br />
                    QThree: {{questionThree}} <br />
                    QFour: {{questionFour}} <br />
                </div>

            </div>
        </p>
    </span>
    {{/each}}
</div>

